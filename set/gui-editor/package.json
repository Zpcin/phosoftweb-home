{
  "name": "about-lang-gui-editor",
  "version": "1.0.0",
  "description": "About-lang.js 图形界面编辑工具",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev": "electron . --dev",
    "build": "electron-builder"
  },
  "keywords": [
    "editor",
    "multilingual",
    "markdown",
    "html",
    "gui"
